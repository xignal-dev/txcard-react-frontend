{
    "name": "txcard-react-frontend",
    "version": "0.2.0",
    "description": "travel express service",
    "scripts": {
        "dev": "next -p 8100",
        "build": "cross-env NODE_OPTIONS='--openssl-legacy-provider' next build",
        "devt": "cross-env NODE_OPTIONS='--openssl-legacy-provider' NEXT_PUBLIC_DEVICE=tablet next -p 8100",
        "devtt": "cross-env NODE_OPTIONS='--openssl-legacy-provider' NEXT_PUBLIC_CENTRAL_API_ENDPOINT=http://183.101.143.204:18589 NEXT_PUBLIC_DEVICE=tablet next -p 8100",
        "start": "next start -p 8100",
        "tablet": "cross-env NEXT_PUBLIC_DEVICE=tablet next start -p 8100",
        "test": "xo && stylelint './pages/**/*.js'"
    },
    "main": "pages/index.js",
    "author": "Keon Lee",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "babel-eslint": "^10.0.3",
        "babel-plugin-styled-components": "^1.10.7",
        "eslint-config-xo-react": "^0.22.0",
        "eslint-plugin-react": "^7.18.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "next-fonts": "^1.0.3",
        "next-offline": "^4.0.6",
        "stylelint": "^13.0.0",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.9.0",
        "xo": "*"
    },
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.21.0",
        "@blueprintjs/core": "^3.23.1",
        "@blueprintjs/icons": "^4.14.5",
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@types/node": "^18.15.12",
        "@types/react": "^18.0.33",
        "auth0-js": "^9.12.2",
        "axios": "^1.3.5",
        "core-js": "^3.6.4",
        "cross-env": "^7.0.3",
        "graphql": "^14.6.0",
        "i18n": "^0.15.1",
        "i18next": "^22.4.14",
        "js-cookie": "^2.2.1",
        "jsonwebtoken": "^8.5.1",
        "mobile-detect": "^1.4.5",
        "mobx": "^6.9.0",
        "mobx-react": "^7.6.0",
        "mobx-state-tree": "^5.1.8",
        "mobx-store-provider": "^2.1.2",
        "moment": "^2.29.4",
        "next": "^9.2.1",
        "next-transpile-modules": "^10.0.0",
        "node-fetch": "^2.6.0",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-calendar": "^4.6.0",
        "react-device-detect": "^2.2.3",
        "react-dom": "^16.12.0",
        "react-i18next": "^12.2.0",
        "react-select": "^5.7.2",
        "react-stripe-checkout": "^2.6.3",
        "react-use-form-state": "^0.12.1",
        "react-uuid": "^2.0.0",
        "styled-components": "^5.0.0",
        "yup": "^0.28.0"
    },
    "xo": {
        "nodeVersion": ">=10",
        "parser": "babel-eslint",
        "envs": [
            "node",
            "browser"
        ],
        "globals": [
            "describe",
            "it",
            "cy",
            "expect",
            "beforeEach"
        ],
        "ignores": "cypress",
        "extends": "xo-react",
        "settings": {
            "react": {
                "version": "16.12"
            }
        },
        "rules": {
            "import/no-unassigned-import": "off",
            "import/no-unresolved": "off",
            "import/no-named-as-default": "off",
            "unicorn/filename-case": "off",
            "unicorn/catch-error-name": "off",
            "react/jsx-sort-props": "off",
            "react/prop-types": "off",
            "react/boolean-prop-naming": "off"
        }
    }
}
